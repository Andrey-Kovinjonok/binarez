{
    // "cquery.cacheDirectory": "${workspaceFolder}/.vscode/cquery_cached_index/",
    // "externalConsole": true,
    "C_Cpp.autocomplete": "default",
    "C_Cpp.autocompleteAddParentheses": true,
    "C_Cpp.errorSquiggles": "enabled",
    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.intelliSenseEngineFallback": "enabled",
    // "C_Cpp.addNodeAddonIncludePaths": true,
    "C_Cpp.workspaceSymbols": "All",
    // "c-cpp-flylint.clang.blocks": false,
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 2, ColumnLimit: 0}",
    "C_Cpp.default.compilerPath": "/usr/bin/gcc",
    "C_Cpp.default.includePath": [
      "../include",
      "/usr/include/c++/14",
      "/usr/lib/gcc/aarch64-linux-gnu/14/include",
      "/usr/include"
    ],
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "xaver.clang-format",

    "editor.rulers": [
      80,
      120
    ],
  
